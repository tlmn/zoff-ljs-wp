    "@prettier/plugin-php": "^0.16.3",
    "@wordpress/scripts": "^16.0.0",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-favicons": "^3.0.0",
    "gulp-postcss": "^9.0.0",
